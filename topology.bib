@article{rs-agsfc-17,
	abstract = {We revisit the classical problem of computing the contour tree of a scalar field {\$}{\$}f:{$\backslash$}mathbb {\{}M{\}}{$\backslash$}rightarrow {$\backslash$}mathbb {\{}R{\}}{\$}{\$}f:M→R, where {\$}{\$}{$\backslash$}mathbb {\{}M{\}}{\$}{\$}Mis a triangulation of a ball in {\$}{\$}{$\backslash$}mathbb {\{}R{\}}\^{}d{\$}{\$}Rd. The contour tree is a fundamental topological structure that tracks the evolution of level sets of f and has numerous applications in data analysis and visualization. All existing algorithms begin with a global sort of at least all critical values of f, which can require (roughly) {\$}{\$}{$\backslash$}Omega (n{$\backslash$}log n){\$}{\$}Ω(nlogn)time, where n is the number of vertices of the mesh. Existing lower bounds show that there are pathological instances where this sort is required. We present the first algorithm whose time complexity depends on the contour tree structure, and avoids the global sort for non-pathological inputs. (We assume that the Morse degree of the function f at any point is at most 3.) If C denotes the set of critical points in {\$}{\$}{$\backslash$}mathbb {\{}M{\}}{\$}{\$}M, the running time is roughly {\$}{\$}O(N+{$\backslash$}sum {\_}{\{}v {$\backslash$}in C{\}} {$\backslash$}log {$\backslash$}ell {\_}v){\$}{\$}O(N+∑v∈Clogℓv), where {\$}{\$}{$\backslash$}ell {\_}v{\$}{\$}ℓvis the depth of v in the contour tree and N is the total complexity of {\$}{\$}{$\backslash$}mathbb {\{}M{\}}{\$}{\$}M. This matches all existing upper bounds, but is a significant asymptotic improvement when the contour tree is short and fat. Specifically, our approach ensures that any comparison made is between nodes that are either adjacent in {\$}{\$}{$\backslash$}mathbb {\{}M{\}}{\$}{\$}Mor in the same descending path in the contour tree, allowing us to argue strong optimality properties of our algorithm. Our algorithm requires several novel ideas: partitioning {\$}{\$}{$\backslash$}mathbb {\{}M{\}}{\$}{\$}Min well-behaved portions, a local growing procedure to iteratively build contour trees, and the use of heavy path decompositions for the time complexity analysis.},
	author = {Raichel, Benjamin and Seshadhri, C.},
	date-added = {2020-08-26 20:40:44 -0500},
	date-modified = {2020-08-26 20:40:44 -0500},
	doi = {10.1007/s00454-017-9901-z},
	journal = {Discrete Comput. Geom.},
	note = {Erratum in \url{https://personal.utdallas.edu/~bar150630/paint.pdf}},
	number = {4},
	pages = {946--985},
	title = {Avoiding the Global Sort: {A} Faster Contour Tree Algorithm},
	volume = {58},
	year = {2017},
	bdsk-url-1 = {https://doi.org/10.1007/s00454-017-9901-z}}

