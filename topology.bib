@inproceedings{ptt-cbnhe-20,
	abstract = {We call a multigraph non-homotopic if it can be drawn in the plane in such a way that no two edges connecting the same pair of vertices can be continuously transformed into each other without passing through a vertex, and no loop can be shrunk to its end-vertex in the same way. It is easy to see that a non-homotopic multigraph on {\$}{\$}n>1{\$}{\$}n>1vertices can have arbitrarily many edges. We prove that the number of crossings between the edges of a non-homotopic multigraph with n vertices and {\$}{\$}m>4n{\$}{\$}m>4nedges is larger than {\$}{\$}c{\backslash}frac{\{}m^2{\}}{\{}n{\}}{\$}{\$}cm2nfor some constant {\$}{\$}c>0{\$}{\$}c>0, and that this bound is tight up to a polylogarithmic factor. We also show that the lower bound is not asymptotically sharp as n is fixed and {\$}{\$}m{\backslash}longrightarrow {\backslash}infty {\$}{\$}m⟶∞.},
	author = {Pach, J{\'a}nos and Tardos, G{\'a}bor and T{\'o}th, G{\'e}za},
	booktitle = {Proc. 28th Int. Symp. Graph Drawing Network Vis.},
	date-added = {2023-02-09 12:01:37 -0600},
