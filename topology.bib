@article{s-otuok-92,
	abstract = {Let S be a compact orientable surface. For any graph G embedded on S and any closed curve D on S we define μG(D) as the minimum number of intersections of G and D′, where D′ ranges over all closed curves freely homotopic to D. We call G a kernel if μG′ ≠ μG for each proper minor G′ of G. We prove that if G and G′ are kernels with μG = μG′ (in such a way that each face of G is an open disk), then G′ can be obtained from G by a series of the following operations: (i) homotopic shifts over S; (ii) taking the surface dual graph; (iii) ΔY-exchange (i.e., replacing a vertex v of degree 3 by a triangle connecting the three vertices adjacent to v, or conversely).
},
	author = {Schrijver, Alexander},
	date-added = {2015-06-27 15:53:29 +0000},
	date-modified = {2015-06-27 15:56:44 +0000},
	journal = {J. Comb. Theory Ser. B},
	keywords = {ΔY, surface graph, homotopy, facewidth},
	pages = {146--160},
	read = {1},
	title = {On the uniqueness of kernels},
	volume = {55},
	year = {1992}}

